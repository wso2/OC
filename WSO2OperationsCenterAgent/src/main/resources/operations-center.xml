<OperationsCenter>
    <Publishers>
  <!-- 
      Real Time Publisher 
    -->
      <Publisher>
        <Name>RTPublisher</Name>
        <Class>org.wso2.carbon.oc.agent.publisher.rt.RTPublisher</Class>
        <Properties>
            <Property name="reportURL">http://localhost:9764/OCServer/services/oc-server/internal/oc</Property>
            <Property name="username">admin</Property>
            <Property name="password">admin</Property>
            <Property name="interval">5000</Property>
        </Properties>
      </Publisher>

